{
  "name": "songiapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@formatjs/cli": "^6.2.4",
    "@mui/icons-material": "^5.14.15",
    "@mui/material": "^5.14.15",
    "@tanstack/react-query": "^5.0.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.6",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "dexie": "^3.2.4",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.5.5",
    "react-router-dom": "^6.17.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "i18n:compile": "formatjs compile \"src/i18n/en-extracted.json\" --out-file src/i18n/en.json",
    "i18n:extract": "formatjs extract \"src/**/!(*.d).ts*\" --out-file src/i18n/en-extracted.json",
    "i18n:missing:cs": "node src/i18n/addMissingKeys.mjs src/i18n/en.json src/i18n/cs.json",
    "i18n": "npm run i18n:extract && npm run i18n:compile && npm run i18n:missing:cs"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
